   "source": [
    "def mult(arr):\n",
    "    result = [1] * len(arr)\n",
    "    i = 0\n",
    "    while i < len(arr):\n",
    "        non_change = result[i]\n",
    "        result = [ x * arr[i] for x in result ]\n",
    "        result[i] = non_change\n",
    "        i += 1\n",
    "    return result    "
   ]
